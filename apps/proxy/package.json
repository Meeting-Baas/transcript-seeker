{
  "name": "@meeting-baas/proxy",
  "private": true,
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "pnpm with-env node dist/index.js",
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env nodemon --exec tsx src/index.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@meeting-baas/shared": "workspace:*",
    "axios": "catalog:",
    "cors": "^2.8.5",
    "dotenv-cli": "catalog:",
    "express": "^4.19.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@meeting-baas/eslint-config": "workspace:*",
    "@meeting-baas/prettier-config": "workspace:*",
    "@meeting-baas/tsconfig": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.11",
    "eslint": "catalog:",
    "http-proxy-middleware": "^3.0.3",
    "nodemon": "^3.1.7",
    "prettier": "catalog:",
    "tsx": "^4.19.1",
    "typescript": "catalog:"
  }
}

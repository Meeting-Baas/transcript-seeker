    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@shikijs/rehype": "^1.22.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "fumadocs-core": "14.2.0",
    "fumadocs-docgen": "^1.3.1",
    "fumadocs-mdx": "11.1.1",
    "fumadocs-ui": "14.2.0",
    "hast": "^1.0.0",
    "hast-util-to-jsx-runtime": "^2.3.2",
    "next": "15.0.1",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-medium-image-zoom": "^5.2.10",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.1",
    "shiki": "^1.22.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@fumadocs/cli": "^0.0.3",
    "@meeting-baas/eslint-config": "workspace:*",
    "@meeting-baas/prettier-config": "workspace:*",
    "@meeting-baas/tailwind-config": "workspace:*",
    "@meeting-baas/tsconfig": "workspace:*",
    "@meeting-baas/ui": "workspace:*",
    "@next/bundle-analyzer": "^15.0.2",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.8.4",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "autoprefixer": "^10.4.20",
    "lucide-react": "^0.453.0",
    "postcss": "^8.4.47",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  }
}

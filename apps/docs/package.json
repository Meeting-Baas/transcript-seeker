{
  "name": "@meeting-baas/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:pre && pnpm with-env next build && pnpm build:post",
    "build:pre": "pnpm with-env tsx ./scripts/pre-build.mts",
    "build:post": "pnpm with-env tsx ./scripts/post-build.mts",
    "clean": "git clean -xdf .cache .source .next .turbo node_modules",
    "dev": "pnpm with-env next dev -p 5174",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start -p 5174",
    "typecheck": "tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@oramacloud/client": "^1.3.18",
    "@radix-ui/react-accordion": "^1.2.1",
